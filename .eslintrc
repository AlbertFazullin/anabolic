{
  "extends": "fs",
  "parser": "babel-eslint",
  "env": {
	"browser": true,
	"node": true,
	"es6": true
  },
  "ecmaFeatures": {
	"arrowFunctions": true
  },
  "plugins": [
	"react"
  ],
  "rules": {
      "comma-dangle": ["error", "always-multiline"],
      "react/no-multi-comp": [2, { "ignoreStateless": true }]
  }
}
